<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.miu.cs.librarysystem.ui.window.LoginWindow">
  <grid id="27dc6" binding="contentPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="92819" binding="topPanel" layout-manager="FlowLayout" hgap="5" vgap="25" flow-align="1">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="3771e" class="javax.swing.JLabel" binding="headingLabel">
            <constraints/>
            <properties>
              <text value="Library System Login"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="568a7" binding="centerPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="93fc7" binding="formPanel" layout-manager="GridLayoutManager" row-count="4" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="0" left="0" bottom="0" right="0"/>
            <constraints/>
            <properties/>
            <border type="none"/>
            <children>
              <grid id="ed3ca" binding="usernamePanel" layout-manager="FlowLayout" hgap="5" vgap="3" flow-align="2">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="a026" class="javax.swing.JLabel" binding="usernameLabel">
                    <constraints/>
                    <properties>
                      <text value="Username: "/>
                    </properties>
                  </component>
                  <component id="3a5ad" class="javax.swing.JTextField" binding="usernameField">
                    <constraints/>
                    <properties>
                      <columns value="10"/>
                      <editable value="true"/>
                    </properties>
                  </component>
                </children>
              </grid>
              <grid id="57792" binding="passwordPanel" layout-manager="FlowLayout" hgap="5" vgap="3" flow-align="2">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="e120f" class="javax.swing.JLabel" binding="passwordLabel">
                    <constraints/>
                    <properties>
                      <text value="Password: "/>
                    </properties>
                  </component>
                  <component id="5257e" class="javax.swing.JPasswordField" binding="passwordField">
                    <constraints/>
                    <properties>
                      <columns value="10"/>
                    </properties>
                  </component>
                </children>
              </grid>
              <grid id="1a036" binding="buttonPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="2">
                <constraints>
                  <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="e06b3" class="javax.swing.JButton" binding="loginButton">
                    <constraints/>
                    <properties>
                      <horizontalAlignment value="0"/>
                      <text value="Login"/>
                    </properties>
                  </component>
                </children>
              </grid>
              <grid id="ccd39" binding="messagePanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="0">
                <constraints>
                  <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="5a4de" class="javax.swing.JLabel" binding="errorMessageLabel">
                    <constraints/>
                    <properties>
                      <text value="Login Failed"/>
                    </properties>
                  </component>
                </children>
              </grid>
            </children>
          </grid>
        </children>
      </grid>
    </children>
  </grid>
</form>
