<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.miu.cs.librarysystem.ui.panel.BookshelfPanel">
  <grid id="27dc6" binding="contentPane" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="4627e" binding="topPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="2f9ba" binding="headingTopPanel" layout-manager="BorderLayout" hgap="0" vgap="15">
            <constraints border-constraint="North"/>
            <properties/>
            <border type="none"/>
            <children/>
          </grid>
          <grid id="d4d56" binding="headingCenterPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="e09d3" class="javax.swing.JLabel" binding="bookshelfLabel">
                <constraints/>
                <properties>
                  <text value="Bookshelf"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
      <grid id="ab1bc" binding="centerPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="df602" binding="toolBarPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints border-constraint="North"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="592cd" class="javax.swing.JTextField" binding="searchField">
                <constraints/>
                <properties>
                  <columns value="10"/>
                </properties>
              </component>
              <component id="86743" class="javax.swing.JButton" binding="searchButton">
                <constraints/>
                <properties>
                  <text value="Search"/>
                </properties>
              </component>
              <component id="d57f" class="javax.swing.JButton" binding="clearSearchButton">
                <constraints/>
                <properties>
                  <text value="Clear"/>
                </properties>
              </component>
              <component id="d85b6" class="javax.swing.JButton" binding="addBookButton">
                <constraints/>
                <properties>
                  <text value="Add"/>
                </properties>
              </component>
              <component id="d24a" class="javax.swing.JButton" binding="copyBookButton">
                <constraints/>
                <properties>
                  <text value="Copy"/>
                </properties>
              </component>
            </children>
          </grid>
          <grid id="b593e" binding="bookTablePanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="none"/>
            <children>
              <scrollpane id="7d585" binding="tableScrollPane">
                <constraints/>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="a5411" class="javax.swing.JTable" binding="bookTable">
                    <constraints/>
                    <properties/>
                  </component>
                </children>
              </scrollpane>
            </children>
          </grid>
        </children>
      </grid>
    </children>
  </grid>
</form>
